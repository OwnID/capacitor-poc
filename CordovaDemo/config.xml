<?xml version='1.0' encoding='utf-8'?>
<widget id="com.ownid.demo.custom" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>CordovaDemo</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <resource-file src="ownIdSdkConfig.json" target="app/src/main/assets/ownIdSdkConfig.json" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <resource-file src="OwnIDConfiguration.plist" target="OwnIDConfiguration.plist"/>
        <preference name="deployment-target" value="14.0" />
        <preference name="pods_ios_min_version" value="14.0" />
        <preference name="SwiftVersion" value="5.0" />
    </platform>
</widget>
